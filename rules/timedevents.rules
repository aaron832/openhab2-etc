// increase the counter at midnight
rule "bedtimeevent_rule"
when
    Time cron "0 0 22 * * ?"
then
    zwav_node6_outdoorlighting_switch.sendCommand(OFF)
end

rule "sunset_rule"
when
	//Time cron "0 0 16 * * ?"
	//This is broken until I upgrade out of 2.1.0-1
	Channel 'astro:sun:astrosun:set#event' triggered END
then
	zwav_node6_outdoorlighting_switch.sendCommand(100)
end
